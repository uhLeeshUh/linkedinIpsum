  "description": "Professional Madlibs for your LinkedIn profile",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently 'npm run dev-frontend' 'npm run dev-backend'",
    "dev-frontend": "webpack-dev-server --config ./src/app/webpack.config.ts --mode development --hot",
    "dev-backend": "NODE_ENV=development nodemon --exec 'ts-node -P tsconfig.json ./src/server'",
    "start": "npm run build-frontend && npm run build-backend",
    "build-frontend": "webpack --config ./src/app/webpack.config.ts --mode production",
    "build-backend": "NODE_ENV=production ts-node -P tsconfig.json ./src/server"
  },
