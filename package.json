{
  "name": "linkedin-ipsum",
  "version": "1.0.0",
  "description": "Professional Madlibs for your LinkedIn profile",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently 'npm run dev-frontend' 'npm run dev-backend'",
    "dev-frontend": "webpack-dev-server --config ./src/app/webpack.config.ts --mode development --hot",
    "dev-backend": "NODE_ENV=development nodemon --exec 'ts-node -P tsconfig.server.json ./src/server'",
    "build": "npm run build-frontend && npm run build-backend",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "build-frontend": "webpack --config ./src/app/webpack.config.ts --mode production",
    "build-backend": "cross-env NODE_ENV=production tsc --project tsconfig.server.json",
    "knex": "ts-node -P tsconfig.json node_modules/.bin/knex",
    "migrate": "npm run knex -- migrate:latest --knexfile=src/server/knex/knexfile.ts",
    "migrate:make": "npm run knex -- migrate:make --knexfile=src/server/knex/knexfile.ts",
    "migrate:rollback": "npm run knex -- migrate:rollback --knexfile=src/server/knex/knexfile.ts",
    "seed:make": "npm run knex -- seed:make --knexfile=src/server/knex/knexfile.ts",
    "seed:run": "npm run knex -- seed:run --knexfile=src/server/knex/knexfile.ts",
    "migrate-test-db": "NODE_ENV=test ts-node -P tsconfig.json ./src/server/helpers/migrate-test-db.ts",
    "test": "NODE_ENV=test npm run migrate-test-db && node node_modules/.bin/jest --runInBand --logHeapUsage --detectOpenHandles"
  },
  "keywords": [],
  "author": "Alicia Underhill",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@types/classnames": "^2.2.10",
    "@types/cssnano": "^4.0.0",
    "@types/express": "^4.17.3",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/lodash": "^4.14.149",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/uuid": "^7.0.2",
    "apollo-boost": "^0.4.7",
    "apollo-server-express": "^2.11.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "css-modules-typescript-loader": "^4.0.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "knex": "^0.20.13",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.2",
    "objection": "^2.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pg": "^7.18.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "style-loader": "^1.1.3",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1"
  }
}
